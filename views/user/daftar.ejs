<h3>Daftar Mahasiswa</h3>
<br>

<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Nama</th>
        <th scope="col">Jurusan</th>
        <th scope="col">Umur</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      <% mahasiswa.forEach(data => { %>         
        <tr>
          <td><%= data.nama %> </td>
          <td><%= data.jurusan %> </td>
          <td><%= data.umur %> </td>
          <td><a class="btn btn-secondary" href="">Edit</a> <a class="btn btn-danger">Hapus</a></td>
        </tr>
        <% }) %>
    </tbody>
  </table>
</div>
